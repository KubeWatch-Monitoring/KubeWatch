{
  "name": "kubewatch",
  "version": "0.1.0",
  "description": "KubeWatch is a monitoring application for Kubernetes.\nIt will keep track of multiple K8S nodes, records performance data and visualize it.\nFurthermore, it should send some kind of notification to the responsible people.",
  "main": "index.ts",
  "scripts": {
    "start": "tsc public/js/*.ts ; ts-node index.ts",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "lint": "tsc --noEmit && eslint .. --ext .ts",
    "coverage": "c8 --reporter=lcov --reporter=cobertura --reporter=text-summary npm test",
    "cypress": "$(npm bin)/cypress open"
  },
  "author": "G03-KubeWatch",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sns": "^3.32.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai-dom": "^0.0.12",
    "@types/chart.js": "^2.9.36",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@types/sinon-stub-promise": "^2.1.7",
    "body-parser": "^1.19.0",
    "chart.js": "^3.7.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.3",
    "express-session": "^1.17.2",
    "express-winston": "4.2.0",
    "moment": "^2.29.2",
    "mongodb": "^4.4.1",
    "node-fetch": "^3.2.4",
    "prometheus-query": "^3.2.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vis-data": "^7.1.4",
    "vis-network": "^9.1.2",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "c8": "^7.11.2",
    "chai": "^4.3.6",
    "chai-dom": "^1.11.0",
    "chai-http": "^4.3.0",
    "cypress": "^9.5.4",
    "eslint": "^8.12.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.2",
    "sinon": "^13.0.1",
    "sinon-stub-promise": "^4.0.0"
  }
}
